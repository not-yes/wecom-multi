{
  "build": {
    "beforeDevCommand": "cd ui && npm run dev",
    "beforeBuildCommand": "cd ui && npm run build",
    "frontendDist": "ui/dist",
    "devUrl": "http://localhost:1420",
    "features": ["gui"]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "wecom-multi-open",
  "mainBinaryName": "wecom-multi-open",
  "version": "0.2.0",
  "identifier": "com.wecom.multiopen",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "trayIcon": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true,
      "menuOnLeftClick": false
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "企业微信多开工具",
        "width": 800,
        "height": 600,
        "minWidth": 600,
        "minHeight": 500,
        "center": true,
        "decorations": true,
        "transparent": false,
        "useHttpsScheme": true
      }
    ]
  }
}